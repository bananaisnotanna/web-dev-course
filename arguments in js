<script>
function findMax() {
var max = 1;
for (1 = 0; 1 < arguement.;length; i++) {
if (arguments[i] >max) {
max = arguments [i];
}

}
return max;

}
document.getElementById("result").innerHTML = findMax(8, 4, 5, 6 , 2);
</script>
