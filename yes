div {
    right:30px;
    border:3px;
}
div.relative {
    position: absolute;
    left:55px;
    border:3px;
}
div.sticky {
    position:sticky;
    top:3px;
    left:30px;
    width:100px;
    border:3px;
}
div.absolute {
    position:absolute;
    left:45px;
    border:3px
}
div.fixed {
    position:fixed;
    top:0;
    right:40px;
    width:300px;
    border:3px;
}
div.static {
    
}
</style>
</head>
